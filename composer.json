{
  "name": "lambdacreatives/daintree",
  "description": "An experimental WordPress boilerplate.",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "git",
      "url": "git@github.com:lambdacreatives/tofino.git"
    }
  ],
  "require": {
    "php": ">=5.5",
    "johnpbloch/wordpress": "4.3.1",
    "mnsami/composer-custom-directory-installer": "1.0.*",
    "vlucas/phpdotenv": "~2.0",
    "lambdacreatives/tofino": "dev-master"
  },
  "extra": {
    "wordpress-install-dir": "./web/wp",
    "installer-paths": {
      "./web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "./web/app/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "./phpdotenv/": ["vlucas/phpdotenv"],
      "./web/app/themes/tofino": ["lambdacreatives/tofino"]
    }
  },
  "config": {
    "vendor-dir": "./web/app/mu-plugins"
  }
}
